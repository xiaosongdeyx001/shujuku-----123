<!--  START SNIPPET: quizValidators -->

<validators>
	<field name="kenSupplierId">
		<field-validator type="required">
			<message>
				<![CDATA[ 请选择供应商. ]]>
			</message>
		</field-validator>
		<field-validator type="int">
			<param name="min">1</param>
			<message>
				<![CDATA[ 请选择供应商. ]]>
			</message>
		</field-validator>
	</field>

	<field name="kenTourNo">
		<field-validator type="requiredstring">
			<message>请输入团号.</message>
		</field-validator>
	</field>
</validators>

