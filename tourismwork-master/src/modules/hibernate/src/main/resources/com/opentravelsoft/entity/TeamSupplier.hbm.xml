<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.opentravelsoft.entity.TeamSupplier" table="TBL_TEAM_CUSTOMER">
    <composite-id name="id" class="com.opentravelsoft.entity.TeamSupplierId">
      <key-property name="customerId" type="int">
        <column name="CUSTOMER_ID" />
      </key-property>
      <key-property name="teamId" type="int">
        <column name="TEAM_ID" />
      </key-property>
    </composite-id>
  </class>
</hibernate-mapping>
