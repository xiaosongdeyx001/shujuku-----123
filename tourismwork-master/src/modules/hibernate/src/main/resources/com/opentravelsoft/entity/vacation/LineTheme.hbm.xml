<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.opentravelsoft.entity.vacation.LineTheme" table="tbl_line_theme">
    <id name="code" type="string">
      <column name="CODE" length="2" />
      <generator class="assigned" />
    </id>
    <property name="cnName" type="string">
      <column name="CN_NAME" length="40" />
    </property>
    <property name="enName" type="string">
      <column name="EN_NAME" length="80" />
    </property>
    <property name="mapFile" type="string">
      <column name="MAP_FILE" length="50" />
    </property>
  </class>
</hibernate-mapping>
