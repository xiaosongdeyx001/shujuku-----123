<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.opentravelsoft.entity.LinePrice" table="TBL_LINE_PRICE">
    <cache usage="read-write" />
    <id name="recNo" type="string">
      <column name="REC_NO" length="8" />
      <generator class="assigned" />
    </id>
    <property name="lineNo" type="string">
      <column name="LINE_NO" length="8" />
    </property>
    <property name="priceKey" type="string">
      <column name="PRICE_KEY" length="1" />
    </property>
    <property name="subject" type="string">
      <column name="SUBJECT" length="80" />
    </property>
    <property name="price" type="big_decimal">
      <column name="PRICE" precision="9" />
    </property>
    <property name="priceOther" type="big_decimal">
      <column name="PRICE_OTHER" precision="9" />
    </property>
    <property name="priceCost" type="big_decimal">
      <column name="PRICE_COST" precision="9" />
    </property>
    <property name="priceContrast" type="big_decimal">
      <column name="PRICE_CONTRAST" precision="9" />
    </property>
    <property name="startDate" type="timestamp">
      <column name="START_DATE" length="19" />
    </property>
    <property name="endDate" type="timestamp">
      <column name="END_DATE" length="19" />
    </property>
    <property name="weekBit" type="string">
      <column name="WEEK_BIT" length="7" />
    </property>
    <property name="subscription" type="big_decimal">
      <column name="SUBSCRIPTION" precision="9" />
    </property>
    <property name="difference" type="big_decimal">
      <column name="DIFFERENCE" precision="9" />
    </property>
    <property name="crUser" type="java.lang.Integer">
      <column name="CR_USER" />
    </property>
    <property name="crDate" type="timestamp">
      <column name="CR_DATE" length="19" />
    </property>
    <property name="opUser" type="java.lang.Integer">
      <column name="OP_USER" />
    </property>
    <property name="opDate" type="timestamp" generated="always">
      <column name="OP_DATE" length="19" />
    </property>
    <property name="cost1" type="big_decimal">
      <column name="COST1" precision="11" />
    </property>
    <property name="cost2" type="big_decimal">
      <column name="COST2" precision="11" />
    </property>
    <property name="cost3" type="big_decimal">
      <column name="COST3" precision="11" />
    </property>
    <property name="cost4" type="big_decimal">
      <column name="COST4" precision="11" />
    </property>
    <property name="cost5" type="big_decimal">
      <column name="COST5" precision="11" />
    </property>
    <property name="cost6" type="big_decimal">
      <column name="COST6" precision="11" />
    </property>
    <property name="cost7" type="big_decimal">
      <column name="COST7" precision="11" />
    </property>
    <property name="cost8" type="big_decimal">
      <column name="COST8" precision="11" />
    </property>
    <property name="cost9" type="big_decimal">
      <column name="COST9" precision="11" />
    </property>
    <property name="cost10" type="big_decimal">
      <column name="COST10" precision="11" />
    </property>
    <property name="cost11" type="big_decimal">
      <column name="COST11" precision="11" />
    </property>
    <property name="cost12" type="big_decimal">
      <column name="COST12" precision="11" />
    </property>
    <property name="remarks" type="string">
      <column name="REMARKS" length="100" />
    </property>
    <property name="currency" type="string">
      <column name="CURRENCY" length="10" />
    </property>
    <property name="roe" type="big_decimal">
      <column name="ROE" precision="7" />
    </property>
    <property name="unitPrice" type="big_decimal">
      <column name="UNIT_PRICE" precision="11" />
    </property>
    <property name="backMoney" type="big_decimal">
      <column name="BACK_MONEY" precision="11" />
    </property>
    <property name="afieldDuty" type="big_decimal">
      <column name="AFIELD_DUTY" precision="11" />
    </property>
  </class>
</hibernate-mapping>
