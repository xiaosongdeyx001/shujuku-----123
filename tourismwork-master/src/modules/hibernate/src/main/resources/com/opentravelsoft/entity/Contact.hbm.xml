<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.opentravelsoft.entity.Contact" table="TBL_CONTACT">
    <id name="contactId" type="java.lang.Integer">
      <column name="CONTACT_ID" />
      <generator class="native" />
    </id>
    <property name="customerId" type="int">
      <column name="CUSTOMER_ID" not-null="true" />
    </property>
    <property name="name" type="string">
      <column name="NAME" length="30" />
    </property>
    <property name="rank" type="string">
      <column name="RANK" length="40" />
    </property>
    <property name="phone" type="string">
      <column name="PHONE" length="40" />
    </property>
    <property name="fax" type="string">
      <column name="FAX" length="40" />
    </property>
    <property name="mobile" type="string">
      <column name="MOBILE" length="20" />
    </property>
    <property name="email" type="string">
      <column name="EMAIL" length="60" />
    </property>
    <property name="msn" type="string">
      <column name="MSN" length="60" />
    </property>
    <property name="skype" type="string">
      <column name="SKYPE" length="60" />
    </property>
    <property name="qq" type="string">
      <column name="QQ" length="30" />
    </property>
    <property name="del" type="string">
      <column name="DEL" length="1" />
    </property>
    <property name="salesId" type="java.lang.Integer">
      <column name="SALES_ID" />
    </property>
    <property name="created" type="timestamp">
      <column name="CREATED" length="19" />
    </property>
    <property name="createdBy" type="java.lang.Integer">
      <column name="CREATEDBY" />
    </property>
    <property name="updated" type="timestamp">
      <column name="UPDATED" length="19" not-null="true" />
    </property>
    <property name="updatedBy" type="java.lang.Integer">
      <column name="UPDATEDBY" />
    </property>

  </class>
</hibernate-mapping>
