<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.opentravelsoft.entity.vacation.LineExtendService" table="tbl_lineextendservice">
    <id name="id" type="java.lang.Integer">
      <column name="ID" />
      <generator class="identity" />
    </id>
    <property name="lineNo" type="string">
      <column name="LineNo" length="10" not-null="true" />
    </property>
    <property name="serviceId" type="int">
      <column name="ServiceID" not-null="true" />
    </property>
    <property name="type" type="int">
      <column name="Type" not-null="true" />
    </property>
  </class>
</hibernate-mapping>
